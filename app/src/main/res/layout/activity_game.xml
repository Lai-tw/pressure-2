<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GameActivity"
    android:background="@drawable/bg12">


    <ImageButton
        android:id="@+id/guess"
        android:layout_width="343dp"
        android:layout_height="107dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="440dp"
        android:background="@drawable/guessiamgebutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/puzzle"
        android:layout_width="343dp"
        android:layout_height="107dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="260dp"
        android:background="@drawable/puzzleiamgebutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/balloon"
        android:layout_width="343dp"
        android:layout_height="107dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/ballooniamgebutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/button4"
        android:layout_width="155dp"
        android:layout_height="72dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="588dp"
        android:background="@drawable/backiamgebutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>